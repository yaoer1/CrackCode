 ListNode *addTwoNumbers(ListNode *l1, ListNode *l2, int sign){
        ListNode *res = new ListNode(sign);
        if(!l1 && !l2) return sign ? res : NULL;
        int value = sign;
        if(l1) value += l1->val;
        if(l2) value += l2->val;
        res->val = value % 10;
        res->next = addTwoNumbers(l1?l1->next:NULL, l2?l2->next:NULL,value > 9);
        return res;
    }
