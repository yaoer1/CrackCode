//p1从head向后遍历，p2从相遇点开始遍历，遍历到相同的节点即为首节点

ListNode *findBeginning(ListNode *head)
{
	if(head == NULL || head->next == head) return head;
	ListNode *p1 = head;	
	ListNode *p2 = head;
	while(p2->next){
		p1 = p1->next;
		p2 = p2->next->next;
		if(p1 == p2) break;
	}
	if(p2->next == NULL) return false;
	while(p1 != p2){
		p1 = p1->next;
		p2 = p2->next;
	}
	return p2;
}
