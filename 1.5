void replace(char *str)
{
	if(str){
		int spaceCount = 0;
		int len = strlen(str);
		for(int i = 0; i < len; i++)
			if(str[i] == ' ') ++spaceCount;
		int newlen = len + 2 * spaceCount;
		str[newlen--] = 0;
		--len;
		while(len < newlen){
			if(str[len] == ' '){
				str[newlen--] = '%';
				str[newlen--] = '0';
				str[newlen--] = '2';
				--len;
			}
			else
				str[newlen--] = str[len--];
		}
	}
}
